{
    "name": "kristories/laravel-rsd",
    "description": "Subdomain validator for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Wahyu Kristianto",
            "email": "w.kristories@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Kristories\\Rsd\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kristories\\Rsd\\Tests\\": "tests"
        }
    },
    "require": {
        "php" : "^7.0",
        "illuminate/config": "~5.5.0|~5.6.0",
        "illuminate/database": "~5.5.0|~5.6.0",
        "illuminate/support": "~5.5.0|~5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2|^7.0",
        "orchestra/testbench": "~3.5.0|~3.6.0"
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kristories\\Rsd\\RsdServiceProvider"
            ]
        }
    }
}
